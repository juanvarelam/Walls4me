        android:supportsRtl="true"
        android:theme="@style/Theme.Walls4me">
        <activity android:name=".AvisoLegalActivity"></activity>
        <activity android:name=".Categorias.VehiculosActivity" />
        <activity android:name=".Categorias.TexturasActivity" />
        <activity android:name=".Categorias.PaisajesActivity" />
        <activity android:name=".Categorias.DeportesActivity" />
        <activity android:name=".Categorias.AnimalesActivity" />
        <activity android:name=".Categorias.AbstractoActivity" />
        <activity android:name=".Models.WallpaperActivity" />
        <activity android:name=".Auth.RecuperaPassActivity" />
        <activity android:name=".Auth.InicialActivity" />
        <activity android:name=".Auth.LoginActivity" />
        <activity android:name=".Auth.RegisterActivity" />
        <activity android:name=".Home.HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>